    {
      "src": "data:image/svg+xml;base64,",
      "sizes": "768x1024",
      "type": "image/svg+xml",
      "form_factor": "wide",
      "label": "Histórico e gráficos detalhados"
    }
  ],
  "icons": [
    {
      "src": "data:image/svg+xml;base64,",
      "sizes": "72x72",
      "type": "image/svg+xml",
      "purpose": "any"
    },
    {
      "src": "data:image/svg+xml;base64,",
      "sizes": "96x96",
      "type": "image/svg+xml",
      "purpose": "any"
    },
    {
      "src": "data:image/svg+xml;base64,",
      "sizes": "128x128",
      "type": "image/svg+xml",
      "purpose": "any"
    },
    {
      "src": "data:image/svg+xml;base64,",
      "sizes": "144x144",
      "type": "image/svg+xml",
      "purpose": "any"
    },
    {
      "src": "data:image/svg+xml;base64,",
      "sizes": "192x192",
      "type": "image/svg+xml",
      "purpose": "any"
    },
    {
      "src": "data:image/svg+xml;base64,",
      "sizes": "512x512",
      "type": "image/svg+xml",
      "purpose": "any maskable"
    }
  ],
  "shortcuts": [
    {
      "name": "Registrar Humor",
      "short_name": "Humor",
      "description": "Registrar rapidamente seu humor atual",
      "url": "/?screen=mood",
      "icons": [
        {
          "src": "data:image/svg+xml;base64,",
          "sizes": "96x96",
          "type": "image/svg+xml"
        }
      ]
    },
    {
      "name": "Ver Histórico",
      "short_name": "Histórico",
      "description": "Visualizar gráficos e estatísticas",
      "url": "/?screen=history",
      "icons": [
        {
          "src": "data:image/svg+xml;base64,",
          "sizes": "96x96",
          "type": "image/svg+xml"
        }
      ]
    },
    {
      "name": "Relatório IA",
      "short_name": "Análise",
      "description": "Gerar relatório com inteligência artificial",
      "url": "/?screen=report",
      "icons": [
        {
          "src": "data:image/svg+xml;base64,",
          "sizes": "96x96",
          "type": "image/svg+xml"
        }
      ]
    }
  ],
  "file_handlers": [
    {
      "action": "/?import",
      "accept": {
        "application/json": [".json", ".mental-backup"]
      },
      "launch_type": "single-client"
    }
  ],
  "protocol_handlers": [
    {
      "protocol": "web+mentalia",
      "url": "/?action=%s"
    }
  ],
  "share_target": {
    "action": "/?share",
    "method": "POST",
    "enctype": "multipart/form-data",
    "params": {
      "title": "title",
      "text": "text",
      "url": "url",
      "files": [
        {
          "name": "backup_file",
          "accept": ["application/json", ".json", ".mental-backup"]
        }
      ]
    }
  },
  "edge_side_panel": {
    "preferred_width": 400
  },
  "display_override": ["window-controls-overlay", "standalone", "minimal-ui"],
  "prefer_related_applications": false,
  "related_applications": [],
  "features": [
    "CrossOriginEmbedderPolicyCredentialless",
    "WebAppUrlHandling"
  ],
  "handle_links": "preferred",
  "launch_handler": {
    "client_mode": "navigate-existing"
  },
  "id": "mental-ia-3",
  "manifest_version": 3,
  "permissions": [
    "storage",
    "notifications",
    "background-sync"
  ],
  "apps": {
    "shortcuts": [
      {
        "name": "Emergency Contact",
        "description": "Quick access to emergency mental health resources",
        "url": "/?emergency=true",
        "icons": [
          {
            "src": "data:image/svg+xml;base64,",
            "sizes": "96x96",
            "type": "image/svg+xml"
          }
        ]
      }
    ]
  },
  "experimental": {
    "auto_launch": false,
    "controlled_frame": false
  }
}