{
  "name": "MentalIA 3.0",
  "short_name": "MentalIA",
  "description": "App de saúde mental com IA local e privacidade absoluta",
  "version": "3.0.0",
  "start_url": "/",
  "display": "standalone",
  "orientation": "portrait-primary",
  "theme_color": "#1a1a2e",
  "background_color": "#0f172a",
  "lang": "pt-BR",
  "scope": "/",
  "categories": ["health", "medical", "lifestyle", "productivity"],
  "screenshots": [
